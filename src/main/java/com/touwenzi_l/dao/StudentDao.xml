<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.touwenzi_l.dao.StudentDao">
	<!--定义sql语言-->
	<select id="selectById" resultType="com.touwenzi_l.pojo.Student">
